  {% schema %}
    {
    "name": "Shop Contact Panel Noosa",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading",
        "placeholder": "Rent A Surfboard"
      },
      {
        "type": "html",
        "id": "google_maps_embed",
        "label": "Google Maps Embed"
      },
      {
        "type": "text",
        "id": "shop_name",
        "label": "Shop Name",
        "placeholder": "Golden Breed - Noosa"
      },
      {
        "type": "text",
        "id": "address",
        "label": "Address",
        "placeholder": "13 Noosa Drive, Noosa Heads QLD 4567"
      },
      {
        "type": "text",
        "id": "hours",
        "label": "Hours",
        "placeholder": "8:30am - 5:30pm"
      },
      {
        "type": "text",
        "id": "phone",
        "label": "Phone",
        "placeholder": "02 6680 7944"
      },
      {
        "type": "text",
        "id": "whatsapp",
        "label": "WhatsApp",
        "placeholder": "02 6680 7944"
      },
      {
        "type": "text",
        "id": "email",
        "label": "Email",
        "placeholder": "GBByronbay@goldenbreed.com.au"
      },
      {
        "type": "textarea",
        "id": "driving_directions",
        "label": "Driving Directions",
        "placeholder": "Lorem ipsum dolor sit amet consectetur adipisicing elit."
      },
      {
        "type": "textarea",
        "id": "parking_information",
        "label": "Parking Information",
        "placeholder": "Lorem ipsum dolor sit amet consectetur adipisicing elit."
      },
      {
        "type": "textarea",
        "id": "wheel_chair_accessibility_information",
        "label": "Wheel chair accessibility information",
        "placeholder": "Lorem ipsum dolor sit amet consectetur adipisicing elit."
      },
      {
        "type": "textarea",
        "id": "pet_friendliness",
        "label": "Pet Friendliness",
        "placeholder": "Lorem ipsum dolor sit amet consectetur adipisicing elit."
      }
    ],
    "presets": [
      {
        "name": "Shop Contact Panel"
      }
    ]
  }  
  {% endschema %}