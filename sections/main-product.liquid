      {% capture product_bullets %}
        <div class="media-gallery-bullets">
          <ul class="media-gallery-list default">
            {% if bullets_metaobjects %}
              {% for bullet in bullets_metaobjects %}
                <li class="item">
                  <svg xmlns="http://www.w3.org/2000/svg" width="26" height="19" fill="none"><g clip-path="url(#a)"><path fill="#265979" d="M.731 11.888c1.38 1.025 1.903 1.168 3.052 2.705 1.882 2.521 1.61 3.567 2.174 4.038.565.471 3.115.86 4.87-1.332 1.757-2.193 4.474-8.24 7.965-11.867 3.49-3.628 5.12-4.674 6.166-5.227 1.045-.553-2.278-.43-8.8 4.653-4.557 3.545-7.755 8.444-7.755 8.444s-.899-2.664-2.927-4.448C3.45 7.071-1.547 10.207.731 11.888Z"/></g><defs><clipPath id="a"><path fill="#fff" d="M.145 0h25v19h-25z"/></clipPath></defs></svg>
                {{ bullet.bullet }}
              </li>
              {% endfor %}

              {% else %}
                {% for bullet in default_bullets_metaobject %}
                  <li class="item">
                    <svg xmlns="http://www.w3.org/2000/svg" width="26" height="19" fill="none"><g clip-path="url(#a)"><path fill="#265979" d="M.731 11.888c1.38 1.025 1.903 1.168 3.052 2.705 1.882 2.521 1.61 3.567 2.174 4.038.565.471 3.115.86 4.87-1.332 1.757-2.193 4.474-8.24 7.965-11.867 3.49-3.628 5.12-4.674 6.166-5.227 1.045-.553-2.278-.43-8.8 4.653-4.557 3.545-7.755 8.444-7.755 8.444s-.899-2.664-2.927-4.448C3.45 7.071-1.547 10.207.731 11.888Z"/></g><defs><clipPath id="a"><path fill="#fff" d="M.145 0h25v19h-25z"/></clipPath></defs></svg>
                    {{ bullet.bullet }}
                </li>
              {% endfor %}
            {% endif %}
          </ul>
        </div>
      {% endcapture %}
    {% endif %}

    {% if hide_bullets != true %}
      <script>
        document.addEventListener('DOMContentLoaded', function () {
          const rioMediaGallery = document.querySelector('.rio-media-gallery .pwzrswiper-wrapper');
          if (!rioMediaGallery) return;

          // Get all slides
          const slides = rioMediaGallery.querySelectorAll('.pwzrswiper-slide');
          if (slides.length < 4) return;

          // Clone the 4th slide (index 3)
          const slideToClone = slides[3];
          const duplicate = slideToClone.cloneNode(true);

          // Remove the id from the duplicate
          duplicate.removeAttribute('id');
          duplicate.classList.add('pwzrswiper-slide--bullets');

          duplicate.style.cursor = '';

          // Remove the <img> tag from the duplicate
          const img = duplicate.querySelector('img');
          if (img) img.remove();

          // Dummy HTML to inject (replace with your Liquid variable if needed)
          var newHtml = `{{ product_bullets }}`

          // Inject the new HTML
          duplicate.insertAdjacentHTML('beforeend', newHtml);

          // Insert the duplicate as the 4th slide (after index 2)
          slides[2].after(duplicate);
        });
      </script>
    {% endif %}

