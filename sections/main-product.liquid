    {% assign default_bullets_metaobject = shop.metafields.custom.default_product_gallery_bullets.value %}


    {% if hide_bullets != true %}

      <style>
        .pwzrswiper-slide--bullets {
          background-color: #F7F6EF;
        }

        .media-gallery-bullets {
          position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          max-width: 235px;
          width: 100%;
        }

        .media-gallery-bullets ul {
          list-style: none;
          gap: 12px;
          display: flex;
          flex-direction: column;
        }

        .media-gallery-bullets li {
